.paging_wrap {
    display: flex;
    flex-direction: row;
    justify-content: center;
    font-size: 16px;
    margin-top: 20px;
}
.paging_btn {
    display: inline-block;
    padding: 8px 6px;
    background:#555555;
    margin-left:10px;
    margin-right: 10px;
    color:#fff;
    border-radius: 8px;
}
.paging_display {
    display:flex;
    flex-direction: row;
    align-items: center;
}
.page_split {
    margin-left:10px;
    margin-right: 10px;
}