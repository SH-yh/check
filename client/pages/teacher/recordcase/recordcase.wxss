@import '../../../res/style/search.wxss';
.teacher-recordcase-container{
    margin-left:10rpx;
    margin-right:10rpx;
}
.teacher-recordcas-wrap{
    height:400px;
}
.teacher-recordcase-header,.recordcase-item{
    display: flex;
    padding-top:10px;
    padding-bottom:10px;
}
.recordcase-case-item{
    flex:1;
    text-align: center;
}
.font-size {
    font-size:14px;
}
.student-id{
    flex: 2;
}
.recordcase-item:nth-child(odd){
    background:#DDD;
}